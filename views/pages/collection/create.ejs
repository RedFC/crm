<%- include('../../layouts/head'); %>
   <%- include('../../layouts/startpagecontent'); %>
<div class="card">
    <div class="card-header d-flex justify-content-between">
       <div class="header-title">
          <h4 class="card-title">Create Collection</h4>
       </div>
    </div>
    <div class="card-body">
                <div class="collapse" id="form-element-1">
                        <div class="card">
                        <kbd class="bg-dark"><pre id="basic-form" class="text-white">
                        </div>
                </div>
                      <!-- Entry Points -->
                      <form method="post" action="/admin/customer/create">
                        <div class="form-group">
                            <div class="input-group mb-4">
                                <div class="input-group-prepend">
                                    <label class="input-group-text" for="inputGroupSelect01">Customer:</label>
                                </div>
                                <select name="customer" id="" class="custom-select">
                                    <% for(var i=0; i < data.length; i++) { %>
                                        <option value="<%= data[i].id %>"><%= data[i].name %></option>
                                    <% } %>
                                </select>
                            </div>
                          </div>
                         <div class="form-group">
                           <label for="Email">Amount:</label>
                           <input type="text" class="form-control" name="email" id="Email">
                         </div>
                         <button type="submit" class="btn btn-primary mr-2">Submit</button>
                      </form>
                      <br>
                      <a href="/admin/dashboard"><button type="submit" class="btn bg-warning">Go To Home</button></a>
                   <!-- End points -->
            </div>
    </div>
         <%- include('../../layouts/endpagecontent'); %>
<%- include('../../layouts/footer'); %>